createTable(Personas)
addCol(Personas, CI, integer, PRIMARY_KEY)
addCol(Personas, Nombre, string, NOT_EMPTY)
addCol(Personas, Apellido, string, NOT_EMPTY)
addCol(Personas, Edad, integer, NOT_EMPTY)
addCol(Personas, Barrio, string, ANY)
addCol(Personas, Calle, string, NOT_EMPTY)
insertInto(Personas, CI:Nombre:Apellido:Edad:Calle, 123:Pepe:Boludo:20:SantaLucia:Pablo)
salir()