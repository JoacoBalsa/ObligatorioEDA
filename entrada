createTable(Personas)
createTable(Trabajadores)
addCol(Personas, CI, integer, PRIMARY_KEY)
addCol(Personas, Nombre, string, NOT_EMPTY)
addCol(Personas, Edad, integer, NOT_EMPTY)
addCol(Personas, Apellido, string, ANY)
insertInto(Personas, CI:Nombre:Edad, 1:Pepe:18)
insertInto(Personas, CI:Nombre:Edad, 2:Carlos:18)
insertInto(Personas, CI:Nombre:Edad, 3:Maria:18)
select(Personas, CI:Nombre, Trabajadores1)
printdatatable(Trabajadores1)
salir()

