createTable(Personas)
addCol(Personas, Nombre, string, ANY)
addCol(Personas, Apellido, string, NOT_EMPTY)
addCol(Personas, Edad, integer, ANY)
addCol(Personas, Barrio, string, NOT_EMPTY)
addCol(Personas, Calle, string, ANY)
addCol(Personas, CI, integer, PRIMARY_KEY)
insertInto(Personas, Apellido:Barrio:CI, Panavox:Pocitos:123)
salir()